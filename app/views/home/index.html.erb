<h1>Tennis API</h1>
<h2>Created By Zak Durbin and Carl Reiser</h2>
<table>
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Country</th>
      <th>Ranking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= player.name %></td>
        <td><%= player.country %></td>
        <td><%= player.ranking %></td>
        <td><%= link_to 'Show', player %></td>
        <td><%= link_to 'Edit', edit_player_path(player) %></td>
        <td><%= link_to 'Destroy', player, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<p><%= link_to 'New Player', new_player_path %></p>
